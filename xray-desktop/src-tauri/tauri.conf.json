{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "xray-desktop",
  "version": "0.1.0",
  "identifier": "com.vpnator.xraydesktop",
  "build": {
    "beforeDevCommand": "npm run dev:web",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build:web",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "xray-desktop",
        "width": 760,
        "height": 560,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": ["xraycp"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "../backend/desktop-bridge.cjs"
    ]
  }
}
